<% content_for :page_title do %>
  <%= Spree.t('komplex.advertisements.edit') %>
<% end %>

<%= form_for [:admin, @advertisement], html: { multipart: true } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links', locals: { f: f, collection_url: admin_advertisements_path, resource_url: admin_advertisement_path(@advertisement) } %>
  </fieldset>
<% end %>
