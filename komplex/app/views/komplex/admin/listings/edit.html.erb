<% content_for :page_title do %>
  <%= Spree.t('komplex.listings.edit') %>
<% end %>

<%= form_for [:admin, @listing] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links', locals: { f: f, collection_url: admin_listings_path, resource_url: admin_listing_path(@listing) } %>
  </fieldset>
<% end %>
